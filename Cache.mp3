      nav {
  display: grid;
  grid-template-columns: repeat(9, auto);
}

.logobox {
  grid-column: 1/3;
}
.Home {
  grid-column: 3/4;
}
.News {
  grid-column: 4/5;
}
.Servers {
  grid-column: 5/6;
}
.Games {
  grid-column: 6/7;
}
.Merch {
  grid-column: 7/8;
}
.AboutUs {
  grid-column: 8/10;
}
/* ------------------------------------------------ */
.picbox{
  display: grid;
  grid-template-columns: 1fr 2rem 1fr;
  grid-template-rows: auto;
}
#MainText{
grid-column: 2/3;
}
.BigLogo{
  grid-column: 3/4;
}